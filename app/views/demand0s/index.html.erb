<% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>"><%= value %></div>
<% end %>
<br/>
<h2>Patient Type 0</h2>
<table  id="demands" class="display">
  <thead>
  <tr>
    <th>Specialty</th>
    <th>Mon</th>
    <th>Tue</th>
    <th>Wed</th>
    <th>Thu</th>
    <th>Fri</th>
    <th>Edit</th>

  </tr>
  </thead>
  <tbody>

  <% @demand0.each do |dem0| %>
      <tr>
        <td><%= Specialty.find(dem0.specialty_id).name %></td>
        <td><%= dem0.Mon %></td>
        <td><%= dem0.Tue %></td>
        <td><%= dem0.Wed %></td>
        <td><%= dem0.Thu %></td>
        <td><%= dem0.Fri %></td>
        <% if current_user.admin? %>
            <td>Edit</td>
        <% end %>
      </tr>
  <% end %>
  </tbody>
</table>


<br/>
<% if current_user.admin? %>
<%= link_to 'Create a new calculation', new_calculation_path %><br>  <br>
<% end %>
<%= link_to 'Back to operating-room planning', operationsplan_start_path %>


